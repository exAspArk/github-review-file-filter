{
  "name": "Github Review File Filter",
  "version": "0.1",
  "manifest_version": 2,

  "description": "This extension filtering files by their extensions",
  "icons": { "128": "active.png" },

  "browser_action": {
    "default_icon": "active.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "http://github.com/*/pull/*/files",
    "https://github.com/*/pull/*/files"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
